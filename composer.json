{
  "name": "girolando/base-component",
  "description": "Girolando - BaseComponent",
  "keywords": [
    "laravel",
    "girolando",
    "componente",
    "animal"
  ],
  "licence": "MIT",
  "authors": [
    {
      "name": "Anderson Nunes",
      "email": "anderson.nuneseth@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support" : "5.1.x",
    "illuminate/routing" : "5.1.x"
  },

  "autoload": {
    "psr-4": {
      "Girolando\\BaseComponent\\": "src/module/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Girolando\\Tests\\BaseComponent\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
